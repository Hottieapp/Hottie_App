<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.hottieapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="11"
        android:targetSdkVersion="18" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/fire_logo"
        android:label="@string/app_name"
        android:theme="@style/AppTheme"
        
         >
        <activity
            android:name="com.example.hottieapp.MainActivity"
            android:label="@string/app_name" android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.hottieapp.GridContestCommonActivity"
            android:label="@string/title_activity_grid_contest_common" 
            android:screenOrientation="portrait">
        </activity>
        <activity
            android:name="com.example.hottieapp.GridItemDisplayDetails"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_grid_item_display_details" >
        </activity>
        <activity
            android:name="com.example.hottieapp.Practice_activity"
            android:screenOrientation="portrait"
            android:label="@string/title_activity_practice_activity" >
        </activity>
        
    </application>

</manifest>
