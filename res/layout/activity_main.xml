<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:background="@drawable/blue_back"
    tools:context=".MainActivity" >

    <ImageButton
        android:id="@+id/girls"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentTop="true"
        android:layout_marginTop="29dp"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/circle1" />

    <ImageButton
        android:id="@+id/fests"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignTop="@+id/girls"
        android:layout_toLeftOf="@+id/girls"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/circle1" />

    <ImageButton
        android:id="@+id/couples"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_alignParentLeft="true"
        android:layout_alignTop="@+id/boys"
        android:contentDescription="@string/hello_world"
        android:src="@drawable/circle1" />
    

    <ImageButton
        android:id="@+id/boys"
        android:layout_width="150dp"
        android:layout_height="150dp"
        android:layout_below="@+id/textView2"
        android:layout_marginTop="50dp"
        android:contentDescription="@string/hello_world"
        android:layout_toRightOf="@+id/couples"
        android:src="@drawable/circle1" />

    <TextView
        android:id="@+id/textView1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView2"
        android:layout_alignBottom="@+id/textView2"
        android:layout_alignRight="@+id/fests"
        android:layout_marginRight="16dp"
        android:text="BOY&apos;S\nCONTEST"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/boys"
        android:layout_alignRight="@+id/boys"
        android:layout_marginBottom="55dp"
        android:layout_marginRight="29dp"
        android:text="FESTS"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/textView3"
        android:layout_alignBottom="@+id/textView3"
        android:layout_alignRight="@+id/textView1"
        android:text="COUPLES"
        android:textAppearance="?android:attr/textAppearanceLarge" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/girls"
        android:layout_alignRight="@+id/textView3"
        android:layout_marginBottom="53dp"
        android:text="GIRL's\nCONTEST"
        android:textAppearance="?android:attr/textAppearanceMedium" />

    <TextView
        android:id="@+id/textView5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_above="@+id/fests"
        android:layout_centerHorizontal="true"
        android:text="HOTTIE APP"
        android:textAppearance="?android:attr/textAppearanceLarge" />

</RelativeLayout>
